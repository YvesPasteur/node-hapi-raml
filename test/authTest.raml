#%RAML 0.8
---
title: Test API
baseUri: https://test.raml.net/api/{version}
version: v1
mediaType:  application/json
resourceTypes:
  - collection:
      get:
        responses:
          200:
            body:
  - collection-item:
      get:
        responses:
          200:
            body:
securitySchemes:
  - jwt:
      type: 'x-jwt'
      describedBy:
        headers:
          Authorization:
            type: string

/test:
  type: collection
  displayName: Users
  get:
  securedBy: [ 'jwt' ]
